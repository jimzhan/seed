{
  "name": "seed",
  "version": "1.0.0",
  "description": "Seed for web application in TypeScript",
  "main": "index.js",
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	},
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "lint": "gts lint",
    "build": "tsc",
    "debug": "nodemon src/server/index.ts",
    "start": "cross-env NODE_ENV=production node build/server/index.js",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimzhan/seed.git"
  },
  "keywords": [],
  "author": "Jim Zhan <jim.zhan@me.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jimzhan/seed/issues"
  },
  "homepage": "https://github.com/jimzhan/seed#readme",
  "dependencies": {
    "config": "^3.3.3",
    "cross-env": "^7.0.3",
    "fastify": "^3.10.1",
    "fastify-nextjs": "^5.1.0",
    "next": "^10.0.5",
    "next-pwa": "^5.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "ava": "^3.15.0",
    "gts": "^3.1.0",
    "husky": "^4.3.8",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
